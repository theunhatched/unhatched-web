{
  "name": "unhatched-web",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "lint": "eslint pages",
    "start": "next start -H 0.0.0.0",
    "start-localhost": "next start",
    "test": "jest --passWithNoTests",
    "type-check": "tsc"
  },
  "dependencies": {
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@philihp/eslint-config": "6.0.2",
    "@tsconfig/node18": "1.0.1",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.4",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.25.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "jest": "29.2.0",
    "typescript": "4.8.4"
  },
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:jest/all",
      "@philihp"
    ],
    "settings": {
      "import/extensions": [
        ".js",
        ".ts"
      ],
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts"
        ]
      },
      "import/resolver": {
        "typescript": {},
        "node": {
          "extensions": [
            ".js",
            ".ts"
          ]
        }
      }
    },
    "rules": {}
  }
}
